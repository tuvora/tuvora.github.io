{
  "name": "tuvora.github.io",
  "version": "1.0.0",
  "description": "Iana Turovschi CV",
  "main": "index.js",
  "scripts": {
    "build": "npm run format && npm run generate-sprite && webpack --mode production",
    "generate-sprite": "svg-sprite -s --symbol --dest src/assets --symbol-dest ./sprite --sprite=sprite.css.svg src/assets/icons/*.svg",
    "start": "webpack serve --open",
    "lint": "eslint src/**/*.{js,html}",
    "lint:fix": "eslint 'src/**/*.{js,html}' --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "format": "prettier --write src/**/*.{js,html,scss}",
    "deploy": "gh-pages -d dist"
  },
  "type": "module",
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.scss": "stylelint --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@eslint/js": "^9.14.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cssnano": "^7.0.6",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.2.0",
    "globals": "^15.11.0",
    "html-webpack-plugin": "^5.5.3",
    "lint-staged": "^15.2.10",
    "micromatch": "^4.0.8",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "sass-loader": "^16.0.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.8.1",
    "svg-sprite": "^2.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "not dead"
  ],
  "dependencies": {
    "core-js": "^3.39.0",
    "terser-webpack-plugin": "^5.3.10"
  }
}
